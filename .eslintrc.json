{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "linebreak-style": 0,
        "eol-last": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "quotes": 0,
        "react/forbid-prop-types": 0,
        "no-alert": 0,
        "no-undef": 0
    }
}